  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "@gfxlabs/oku-chains": "^1.1.161",
    "fs": "^0.0.1-security",
    "fs.promises": "^0.1.2",
    "path": "^0.12.7",
    "sharp": "^0.33.5",
    "typescript": "^5.7.3",
    "viem": "^2.22.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "prettier": "^3.4.2"
  }
}
